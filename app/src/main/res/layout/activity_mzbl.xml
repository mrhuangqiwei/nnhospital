<?xml version="1.0" encoding="utf-8"?>
<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.qiwei.hospital.ui.MzblActivity">
    <RelativeLayout
        android:id="@+id/zyfy_title"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#FF8746"

        >
        <ImageButton
            android:paddingLeft="5dp"
            android:id="@+id/img_mzfy_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@drawable/onclick_land_back"
            android:src="@mipmap/uc_land_back"/>
        <TextView

            android:id="@+id/tv_mzbl_title"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="@string/tv_mzbl_title"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>


 <LinearLayout
        android:id="@+id/zyfy_ll_title"
        android:background="@drawable/common_re_ly"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"
     android:orientation="vertical"
        >

     <FrameLayout

         android:id="@+id/frm_jiben"
         android:layout_width="match_parent"
         android:layout_height="50dp">
         <TextView
             android:id="@+id/tv_mzbl_jbxx"
             android:visibility="gone"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:textSize="25sp"
             android:textColor="@color/dodgerblue"
             android:text="@string/tv_mzbl_jiben"
             android:layout_gravity="center"
             android:gravity="center"/>

     <RelativeLayout
         android:visibility="visible"
         android:id="@+id/re_mzbl_chx"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:layout_marginLeft="10dp"
         android:layout_marginTop="5dp"
         android:background="@drawable/common_re_ly"
         >

         <TextView
             android:id="@+id/mzfy_mzbl_sf"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_centerVertical="true"
             android:layout_marginLeft="5dp"
             android:textSize="20sp"
             android:text="@string/mzfy_tv_ghxh"

             />

         <EditText
             android:id="@+id/mzbl_tv_brrygh"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_centerHorizontal="true"
             android:textSize="18sp"

             android:layout_marginBottom="1dp"
             android:layout_marginLeft="5dp"
             android:layout_toRightOf="@+id/mzfy_mzbl_sf"
             android:layout_toLeftOf="@+id/mzbl_btn_ghxh"
             />
         <ImageButton
             android:id="@+id/mzbl_btn_ghxh"
             android:layout_width="40dp"
             android:layout_marginRight="20dp"
             android:layout_height="30dp"
             android:src="@mipmap/img_chaxun"
             android:layout_alignParentRight="true"
             android:background="@drawable/onclick_zyff_img"
             android:layout_alignParentBottom="true"
             android:layout_alignParentTop="true" />
     </RelativeLayout>
     </FrameLayout>


<LinearLayout
    android:id="@+id/re_mzbl_jbxx"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical">
    <RelativeLayout

        android:id="@+id/re_mzbl_brxm"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="15dp">

        <TextView
            android:id="@+id/mzbl_tv_br"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="15sp"
            android:layout_marginLeft="5dp"
            android:text="@string/zyfy_tv_br"

            />

        <TextView
            android:id="@+id/mzbl_tv_brxm"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:textSize="15sp"

            android:layout_marginTop="12dp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/mzbl_tv_br"
            />
        <TextView
            android:layout_centerVertical="true"
            android:id="@+id/mzbl_tv_ghk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mzbl_tv_gh"
            android:textSize="15sp"
            android:layout_toRightOf="@+id/mzbl_tv_brxm"
            android:layout_marginLeft="15dp"

            />

        <TextView
            android:layout_centerVertical="true"
            android:id="@+id/mzbl_tv_ghks"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/mzbl_tv_ghk"
            android:textSize="15sp"
            android:layout_marginLeft="5dp"
            />
    </RelativeLayout>


    <RelativeLayout

        android:id="@+id/re_mzbl_jz"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp">

        <TextView
            android:id="@+id/mzbl_tv_ys"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="15sp"
            android:layout_marginLeft="5dp"
            android:text="@string/mzfy_tv_ghys"

            />

        <TextView
            android:id="@+id/mzbl_tv_jzys"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:textSize="15sp"

            android:layout_marginTop="12dp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/mzbl_tv_ys"
            />
        <TextView
            android:layout_centerVertical="true"
            android:id="@+id/mzbl_tv_jz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/mzfy_tv_jzrq"
            android:textSize="15sp"
            android:layout_toRightOf="@+id/mzbl_tv_jzys"
            android:layout_marginLeft="15dp"

            />

        <TextView
            android:layout_centerVertical="true"
            android:id="@+id/mzbl_tv_jzsj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/mzbl_tv_jz"
            android:textSize="15sp"
            android:layout_marginLeft="5dp"
            />
    </RelativeLayout>


    <RelativeLayout

        android:id="@+id/re_mzbl_nl"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp">

        <TextView
            android:layout_marginLeft="5dp"
            android:id="@+id/mzbl_tv_sf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/zyfy_tv_sf"
            android:textSize="15sp"
            />

        <TextView
            android:layout_centerVertical="true"
            android:id="@+id/mzbl_tv_sfzh"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/mzbl_tv_sf"
            android:textSize="12sp"
            android:layout_marginLeft="15dp"
            />
        <TextView


            android:id="@+id/mzbl_tv_ln"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="15sp"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@id/mzbl_tv_sfzh"
            android:text="@string/tv_mzbl_nl"

            />

        <TextView
            android:id="@+id/mzbl_tv_brnl"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:textSize="15sp"

            android:layout_marginTop="12dp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/mzbl_tv_ln"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_below="@+id/re_mzbl_jzrq"
        android:id="@+id/re_mzfy_jtzj"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="15dp">

        <TextView
            android:id="@+id/mzfy_tv_mzbl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="15sp"
            android:layout_marginLeft="5dp"
            android:text="@string/mzfy_tv_jtzj"

            />

        <TextView
            android:id="@+id/mzbl_tv_brjtzj"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:textSize="15sp"

            android:layout_marginTop="12dp"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/mzfy_tv_mzbl"
            />
    </RelativeLayout>

</LinearLayout>


</LinearLayout>

<ListView
    android:id="@+id/mzbl_ll_cfd"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="2dp"

    >

</ListView>



        </LinearLayout>


